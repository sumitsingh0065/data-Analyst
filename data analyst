import spacy
nlp = spacy.load ('en_core_web_sm')
text = nlp('Customizable speech-specific sentence tokenizer that allows for unlimited lengths of text to be read, all while keeping proper ')
for i in text.sents:
    print(i)
for i in text:
    print(i)
from nltk.tokenize import sent_tokenize
import nltk
nltk.download('punkt')
nltk.download('punkt_tab')
sent_tokenize('i am sumit, i am paras')
nltk.download('punkt_tab')
